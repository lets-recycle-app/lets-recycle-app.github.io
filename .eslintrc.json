{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "jest": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js"
        ]
      }
    }
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "react/prop-types": "off",
    "react/jsx-key": "off",
    "no-console": "off",
    "max-len": 0,
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "always"
      }
    ]
  }
}